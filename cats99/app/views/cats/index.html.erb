CATS EVERYWHERE

<ul>
  <% @cats.each do |cat| %>
  <li>  
    name: <%= cat.name %>
  </li><li>
    color: <%= cat.color %>
  </li><li>
    sex: <%= cat.sex %>
  </li><li>
    birth_date: <%= cat.birth_date %>
  </li><li>
    description: <%= cat.description %>
  </li><li>
    id: <%= cat.id %>
  </li><li>
    <a href="<%= cat_url(cat) %>">Show Cat</a>
  </li><br><br>
  <% end %>
</ul>